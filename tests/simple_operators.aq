fn main()
{
    var a = 1 + 1; //2
    var b = -1 + 1; //0
    var c = (10 * 2) * -(15 + 5); // -400
    var d = 1 & 1; // 1
    var e = 1 & 0; // 0
    var f = 1 | 0; // 1
    var g = 1 ^ 0; // 1
    var h = 1 ^ 1; // 0
    var i = ~10;   // -11
    
    println(a);
    println(b);
    println(c);
    println(d);
    println(e);
    println(f);
    println(g);
    println(h);
    println(i);
        
}